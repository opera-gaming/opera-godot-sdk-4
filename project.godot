;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Opera SDK with Demo"
run/main_scene="res://demo/scenes/scene.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GxGames="*res://addons/opera_sdk/RuntimeSDK/Public/GxGames.gd"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="keep_width"

[editor_plugins]

enabled=PackedStringArray("res://addons/opera_sdk/plugin.cfg")
